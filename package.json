{
  "name": "safetimeout",
  "version": "0.1.0",
  "description": "A Node.js module that wraps `setTimeout`, `clearTimeout`, `setInterval`, and `clearInterval` to safely operate on delays larger than the max 32-bit integer (2147483647).",
  "main": "index.js",
  "scripts": {
    "test": "grunt travis --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/JamesMGreene/node-safetimeout.git"
  },
  "keywords": [
    "setTimeout",
    "clearTimeout",
    "setInterval",
    "clearInterval",
    "MAX_INT32",
    "32-bit",
    "integer",
    "Number.MAX_VALUE",
    "2147483647",
    "2147483648",
    "safe",
    "overflow"
  ],
  "author": {
    "name": "James M. Greene",
    "email": "james.m.greene@gmail.com",
    "url": "http://jamesgreene.net/"
